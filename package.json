{
  "name": "@schibsted/react-account",
  "version": "1.2.0",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/schibsted/react-account.git"
  },
  "description": "React Context Provider and Hook making it easier to use Schibsted Account in your React app",
  "keywords": [
    "account"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.js"
  },
  "author": "Sverre Norberg-Schulz Hagen",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@rollup/plugin-babel": "^5.3.1",
    "@semantic-release/git": "^10.0.1",
    "rollup": "^2.79.0"
  },
  "peerDependencies": {
    "@schibsted/account-sdk-browser": ">=4.6.0",
    "react": ">=17"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "package-lock.json"
          ],
          "message": "release(version): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ],
    "tagFormat": "${version}"
  }
}
